pluginpaths.Zedo_Resource = "Zedo/Resource"
resources.doctrine2.adapter = "pdo_mysql"
resources.doctrine2.modelsPath = APPLICATION_PATH "/models";

[testing : production]
phpSettings.display_startup_errors = 1
phpSettings.display_errors = 1

